
  (import "../list.qrk")
  (import "operators.qrk")
  (let split (fn (string delimitor) {
    (let result (list))
    (let tmp "")
    (foreach string (fn (item) {
      (if (= item delimitor)
        {
          (push result tmp)
          (set tmp "")
        }
        (set tmp (++ tmp item))
      )
    }))
    (if (!= tmp none) (push result tmp))
    (return result)
  }))
